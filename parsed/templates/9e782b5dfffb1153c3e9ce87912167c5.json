{"components/blog/post_blog":"<div class=\"col-12 col-md-6 col-lg-4 mb-5\">\n    <div class=\"b-post\">\n        <div class=\"b-post-content\">\n            {{#if post.thumbnail}}\n                <div class=\"blog-thumbnail\" style=\"background-image:url({{getImage post.thumbnail 'defaultImage'}})\"><a href=\"{{post.url}}\"></a></div>\n            {{else}}\n                <div class=\"blog-thumbnail\" style=\"background-image:url(https://via.placeholder.com/600x400?text=Blog)\">\n                    <a href=\"{{post.url}}\"></a>\n                </div>\n            {{/if}}\n            <div class=\"b-post-desc\">\n                <h3 class=\"b-post-title\"><a href=\"{{post.url}}\">{{post.title}}</a></h3>\n                <div class=\"b-blog-info\">\n                    <p class=\"b-post-author\"><i class=\"fa fa-user\" aria-hidden=\"true\"></i> {{post.author}} <i class=\"fa fa-calendar\" aria-hidden=\"true\"></i>\n    </p>\n    <p class=\"b-post-date\">{{post.date_published}}</p>\n                    <p class=\"b-blog-summary\">{{post.summary}}&hellip;</p>\n                </div>\n                <a href=\"{{url}}\" class=\"button button--primary\">Read More</a>\n            </div>\n        </div>\n    </div>\n</div>\n"}
