{"components/epicContent/home-testimonials":"<div class=\"testimonial-section\" data-banner-location=\"bottom\">\n    <div class=\"container container-lg\">\n        <div class=\"section-heading\">\n            <h2 class=\"section-title\"><span>{{lang 'epicContent.testimonial.testimonial_title'}}</span></h2>\n            <p class=\"section-subtitle\">{{lang 'epicContent.testimonial.testimonial_description'}}</p>\n        </div>\n        <section    data-equalizer class=\"testimonial-content\" \n                    data-slick='{\n                        \"dots\": false,\n                        \"infinite\": true,\n                        \"slidesToScroll\": 1,\n                        \"slidesToShow\": 2,\n                        \"autoplay\": true,\n                        \"autoplaySpeed\": 4000,\n                        \"speed\": 600,\n                        \"slide\": \".js-testimonial-slide\",\n                        \"prevArrow\": \".js-testimonial-prev-arrow\",\n                        \"nextArrow\": \".js-testimonial-next-arrow\",\n                        \"pauseOnHover\": true,\n                        \"responsive\": [\n                            {\n                            \"breakpoint\": 767,\n                            \"settings\": {\n                                \"slidesToScroll\": 1,\n                                \"slidesToShow\": 1\n                                }\n                            },\n                            {\n                            \"breakpoint\": 991,\n                            \"settings\": {\n                                \"slidesToScroll\": 1,\n                                \"slidesToShow\": 2\n                                }\n                            }\n                            ]\n                        }'\n        >\n        {{#if banners.bottom.length '>' 2}}\n        <button aria-label=\"{{banners.bottom.length }}\" class=\"js-testimonial-prev-arrow slick-prev slick-arrow\"></button>\n        {{/if}}\n            {{#each banners.bottom}}\n                <div class=\"testimonial-item js-testimonial-slide\" data-equalizer-watch>\n                    <div class=\"testimonial-item-content\">\n                        {{{this}}}\n                    </div>\n                </div>\n            {{/each}}\n        {{#if banners.bottom.length '>' 2}}\n            <button aria-label=\"{{banners.bottom.length }}\" class=\"js-testimonial-next-arrow slick-next slick-arrow\"></button>\n        {{/if}}\n        </section>\n    </div>\n</div>\n"}
