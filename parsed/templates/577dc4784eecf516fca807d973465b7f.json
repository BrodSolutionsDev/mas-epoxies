{"components/common/store-logo":"<a href=\"{{urls.home}}\" class=\"header-logo__link\">\n    {{#if settings.store_logo.image}}\n        {{#if theme_settings.logo_size '===' 'original'}}\n            <img class=\"header-logo-image-unknown-size\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n        {{else}}\n            <div class=\"header-logo-image-container\">\n                <img class=\"header-logo-image\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n            </div>\n        {{/if}}\n    {{else}}\n        <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\n    {{/if}}\n</a>\n","components/epicHeader/logo-bar":"<div class=\"container\">\n    {{#if theme_settings.logo-position '==' 'left'}}\n        <div class=\"row align-items-center\">\n            <div class=\"col-md-3 col-lg-3\">\n                {{#if page_type '===' 'default'}}\n                    <h1 class=\"site-logo header-logo--left\">\n                        {{> components/common/store-logo}}\n                    </h1>\n                {{else}}\n                    <div class=\"site-logo header-logo--left\">\n                        {{> components/common/store-logo}}\n                    </div>\n                {{/if}}\n            </div>\n            <div class=\"{{#if theme_settings.left-category-menu}}col-md-7{{else}}col-md-6{{/if}} ml-auto\">\n                {{> components/epicHeader/search}}\n            </div>\n            {{#if settings.show_wishlist}}\n            <div class=\"col-auto{{#if theme_settings.left-category-menu '!==' true}} p-0{{/if}}\">\n                <ul class=\"logobar-nav\">\n                    <li>\n                        <a href=\"{{urls.account.wishlists.all}}\">\n                            <i class=\"fal fa-heart-circle\"></i>\n                            {{lang 'account.nav.wishlists' num_wishlists=customer.num_wishlists}}\n                        </a>\n                    </li>\n                </ul>\n            </div>\n            {{/if}}\n            {{#if theme_settings.left-category-menu '!==' true}}\n                {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\n                    <div class=\"col-auto\">\n                        <div class=\"cart-section\">\n                            <div class=\"cart-items\">\n                                <span class=\"cart-quantity cart-items-count\">{{cart.quantity}}</span>\n                                <span class=\"icon\">\n                                    <svg><use xlink:href=\"#icon-shopping-cart\" /></svg> \n                                </span>\n                            </div>\n                            <div class=\"cart-subtotal\">\n                                <a\n                                    class=\"view-cart\"\n                                    data-cart-preview\n                                    data-dropdown=\"cart-preview-dropdown\"\n                                    href=\"{{urls.cart}}\" aria-label=\"{{lang 'cart.nav_aria_label'}}\">\n                                   <span>My Cart</span><br><span class=\"ct-price\">{{cart.grand_total.formatted}}</span> <i class=\"icon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\n                                </a>\n                                <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\n                            </div>\n                        </div>\n                    </div>\n                    {{else}}\n                {{/or}}\n            {{/if}}\n        </div><!--row-->\n    {{/if}}\n    {{#if theme_settings.logo-position '==' 'center'}}\n    <div class=\"row align-items-center\"> \n            <div class=\"col-lg-4 offset-lg-4\">\n                {{#if page_type '===' 'default'}}\n                    <h1 class=\"site-logo text-center\">\n                        {{> components/common/store-logo}}\n                    </h1>\n                {{else}}\n                    <div class=\"site-logo text-center\">\n                        {{> components/common/store-logo}}\n                    </div>\n                {{/if}} \n            </div>\n            {{#if settings.show_wishlist}}\n            <div class=\"col-auto ml-auto{{#if theme_settings.left-category-menu '!==' true}} p-0{{/if}}\">\n                <ul class=\"logobar-nav\">\n                    <li>\n                        <a href=\"{{urls.account.wishlists.all}}\">\n                            <i class=\"fal fa-heart-circle\"></i>\n                            {{lang 'account.nav.wishlists' num_wishlists=customer.num_wishlists}}\n                        </a>\n                    </li>\n                </ul>\n            </div>\n            {{/if}}\n            {{#if theme_settings.left-category-menu '!==' true}}\n                {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\n                    <div class=\"col-auto\">\n                        <div class=\"cart-section\">\n                            <div class=\"cart-items\">\n                                <span class=\"cart-quantity cart-items-count\">{{cart.quantity}}</span>\n                                <span class=\"icon\">\n                                    <svg><use xlink:href=\"#icon-shopping-cart\" /></svg> \n                                </span>\n                            </div>\n                            <div class=\"cart-subtotal\">\n                                <a\n                                    class=\"view-cart\"\n                                    data-cart-preview\n                                    data-dropdown=\"cart-preview-dropdown\"\n                                    href=\"{{urls.cart}}\" aria-label=\"{{lang 'cart.nav_aria_label'}}\">\n                                   <span>My Cart</span><br><span class=\"ct-price\">{{cart.grand_total.formatted}}</span> <i class=\"icon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\n                                </a>\n                                <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\n                            </div>\n                        </div>\n                    </div>\n                    {{else}}\n                {{/or}}\n            {{/if}}\n        </div><!--row-->\n    {{/if}}\n    {{#if theme_settings.logo-position '==' 'right'}}\n        <div class=\"row align-items-center\">\n            {{#if theme_settings.left-category-menu '!==' true}}\n                {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\n                    <div class=\"col-auto\">\n                        <div class=\"cart-section\">\n                            <div class=\"cart-items\">\n                                <span class=\"cart-quantity cart-items-count\">{{cart.quantity}}</span>\n                                <span class=\"icon\">\n                                    <svg><use xlink:href=\"#icon-shopping-cart\" /></svg> \n                                </span>\n                            </div>\n                            <div class=\"cart-subtotal cart-subtotal-left\">\n                                <a\n                                    class=\"view-cart\"\n                                    data-cart-preview\n                                    data-dropdown=\"cart-preview-dropdown\"\n                                    href=\"{{urls.cart}}\" aria-label=\"{{lang 'cart.nav_aria_label'}}\">\n                                   <span>My Cart</span><br><span class=\"ct-price\">{{cart.grand_total.formatted}}</span> <i class=\"icon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\n                                </a>\n                                <div class=\"dropdown-menu cart-p-right\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\n                            </div>\n                        </div>\n                    </div>\n                    {{else}}\n                {{/or}}\n            {{/if}}\n            {{#if settings.show_wishlist}}\n            <div class=\"col-auto{{#if theme_settings.left-category-menu '!==' true}} p-0{{/if}}\">\n                <ul class=\"logobar-nav\">\n                    <li>\n                        <a href=\"{{urls.account.wishlists.all}}\">\n                            <i class=\"fal fa-heart-circle\"></i>\n                            {{lang 'account.nav.wishlists' num_wishlists=customer.num_wishlists}}\n                        </a>\n                    </li>\n                </ul>\n            </div>\n            {{/if}}\n            <div class=\"{{#if theme_settings.left-category-menu}}col-md-6{{else}}col-md-5{{/if}} mr-auto\">\n                {{> components/epicHeader/search}}\n            </div>\n            <div class=\"col-md-3 col-lg-3\">\n                {{#if page_type '===' 'default'}}\n                    <h1 class=\"site-logo header-logo--right\">\n                        {{> components/common/store-logo}}\n                    </h1>\n                {{else}}\n                    <div class=\"site-logo header-logo--right\">\n                        {{> components/common/store-logo}}\n                    </div>\n                {{/if}}\n            </div>\n        </div><!--row-->\n    {{/if}}\n    {{#if theme_settings.logo-position '!==' 'center'}}\n        <section class=\"quickSearchResults quick-search\" data-bind=\"html: results\"></section>\n    {{/if}}\n</div><!--container-->\n","components/epicHeader/search":"<div class=\"header-search\">\n    <div class=\"container\">\n    <form class=\"form\" action=\"{{urls.search}}\">\n        <div class=\"search-input-field\">\n            <input class=\"search_\" name=\"search_query_adv\" data-search-quick type=\"search\" autocomplete=\"off\" onfocus=\"this.placeholder=''\" onblur=\"this.placeholder='{{theme_settings.header-search-text}}'\" placeholder=\"{{theme_settings.header-search-text}}\">\n            <button  class=\"button button--primary\" name=\"go\" value=\"Search for Resin, Projects and More...\">\n                <i class=\"fal fa-search\"></i>\n            </button>\n        </div>\n    </form>\n    {{#if theme_settings.logo-position '==' 'center'}}\n        <section class=\"quickSearchResults quick-search\" data-bind=\"html: results\"></section>\n    {{/if}}\n    </div>\n</div>\n"}
