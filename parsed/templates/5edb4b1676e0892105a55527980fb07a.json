{"components/common/social-links":"{{#if social_media}}\r\n    <ul class=\"socialLinks socialLinks--alt\">\r\n        {{#each social_media}}\r\n            <li class=\"socialLinks-item\">\r\n                <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\" rel=\"noopener\">\r\n                    <svg><use xlink:href=\"#icon-{{display_name}}\" /></svg>\r\n                </a>\r\n            </li>\r\n        {{/each}}\r\n        <li class=\"socialLinks-item\">\r\n            <a class=\"icon icon--tiktok\" href=\"https://www.tiktok.com/@masepoxies?\" target=\"_blank\">\r\n                <svg aria-hidden=\"true\" focusable=\"false\" data-prefix=\"fab\" data-icon=\"tiktok\" class=\"svg-inline--fa fa-tiktok fa-w-14\" role=\"img\" xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 448 512\"><path d=\"M448,209.91a210.06,210.06,0,0,1-122.77-39.25V349.38A162.55,162.55,0,1,1,185,188.31V278.2a74.62,74.62,0,1,0,52.23,71.18V0l88,0a121.18,121.18,0,0,0,1.86,22.17h0A122.18,122.18,0,0,0,381,102.39a121.43,121.43,0,0,0,67,20.14Z\"></path></svg>\r\n            </a>\r\n        </li>\r\n    </ul>\r\n{{/if}}\r\n","components/epicContent/newsletter-popup":"<div id=\"epic-newsletter-popup\" class=\"d-none newsletter-popup\">\n    <div class=\"newsletter-popup-container align-items-center\">\n        <div class=\"newsletter-popup-inner\">\n            <button title=\"Close\" type=\"button\" class=\"popup-close\" data-close-newsletter-popup>\n                <i class=\"fal fa-times-circle\"></i>\n            </button>\n            <div class=\"row no-gutters align-items-center h-100\">\n                {{#if theme_settings.np-image \"!==\" \"\"}}\n                    <div class=\"col-12 col-md-4 d-none d-md-block h-100\">\n                        <img class=\"img-responsive\" src=\"{{getImage theme_settings.np-image}}\" alt=\"\">\n                    </div>\n                {{/if}}\n                <div class=\"col-12 col-md\">\n                    <div class=\"newsletter-popup-content\">\n                        <h4>{{lang 'newsletter.subscribe'}}</h4>\n                        <p>{{lang 'newsletter.subscribe_intro'}}</p>\n                        <div class=\"popup-subcribe-form\">\n                            <form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\" id=\"popupNewsletterForm\">\n                                <fieldset class=\"form-fieldset\">\n                                    <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n                                    <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\n                                    <input type=\"hidden\" name=\"check\" value=\"1\">\n                                    <div class=\"form-field\">\n                                        <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\n                                        <div class=\"form-wrap\">\n                                            <input class=\"form-input\" id=\"nl_email\" name=\"nl_email\" type=\"email\" value=\"{{customer.email}}\" placeholder=\"{{lang 'newsletter.email_placeholder'}}\">\n                                            <input class=\"button button--primary\" type=\"submit\" value=\"{{lang 'newsletter.subscribe_submit'}}\">\n                                        </div>\n                                    </div>\n                                </fieldset>\n                            </form>\n                        </div>\n                        <div class=\"popup-social-icons\">\n                            <h5>{{lang 'social.connect'}}</h5>\n                            {{> components/common/social-links}}\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</div>\n"}
